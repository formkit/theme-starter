{
  "name": "@formkit/theme-starter",
  "version": "2.0.2",
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/formkit/theme-starter.git"
  },
  "author": "",
  "license": "MIT",
  "main": "dist/theme.cjs",
  "module": "dist/theme.js",
  "types": "dist/theme.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/theme.d.ts",
        "default": "./dist/theme.js"
      },
      "require": {
        "types": "./dist/theme.d.cts",
        "default": "./dist/theme.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsup",
    "build-web": "vite build",
    "preview": "vite preview",
    "release": "pnpm build && npx publint && npx bumpp --push",
    "test": "vitest",
    "lint": "eslint"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@formkit/theme-creator": "latest"
  },
  "devDependencies": {
    "@formkit/addons": "latest",
    "@formkit/barcode": "latest",
    "@formkit/icons": "latest",
    "@formkit/pro": "next",
    "@formkit/theme-editor": "latest",
    "@formkit/vue": "latest",
    "@eslint/js": "^9.39.1",
    "@vitejs/plugin-vue": "^4.6.2",
    "bumpp": "^9.3.0",
    "eslint": "^9.39.1",
    "typescript-eslint": "^8.32.1",
    "prettier": "^3.2.5",
    "publint": "^0.2.7",
    "tailwindcss": "^4.0.0",
    "tsup": "^8.5.1",
    "typescript": "^5.3.3",
    "vite": "^4.5.2",
    "vitest": "^0.34.6",
    "vue": "^3.4.15",
    "vue-tsc": "^1.8.27"
  }
}
